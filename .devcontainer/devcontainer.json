{
  "dockerComposeFile": "./docker-compose.yml",
  "service": "devcontainer",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "features": {
    "ghcr.io/devcontainers/features/java": {
      "version": "21",
      "installMaven": true,
      "mavenVersion": "latest"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "cweijan.vscode-myssql-client2"
      ]
    }
  }
}
